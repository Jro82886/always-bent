{
  "name": "always-bent",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node scripts/dev-guard.js && next dev",
    "clean:start": "bash scripts/clean-start.sh",
    "env:backup": "bash scripts/env-backup.sh",
    "env:restore": "bash scripts/env-restore.sh",
    "polys:rebuild": "echo 'deprecated: build_copernicus_sst_polys.py removed' && exit 0",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --max-warnings=0",
    "typecheck": "tsc -p . --noEmit",
    "prepare": "husky",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "type-check:quick": "node scripts/type-check-quick.js",
    "lint:inlet": "bash scripts/lint-inlet.sh",
    "lint:contracts": "bash scripts/lint-contracts.sh",
    "depcheck": "depcheck --ignores=@types/*,eslint*,tailwindcss,postcss,autoprefixer",
    "tsprune": "ts-prune -g -P tsconfig.json",
    "analyze": "npm run lint && npm run typecheck && npm run depcheck && npm run tsprune",
    "check:routes": "node scripts/check-routes.js",
    "check:guardrails": "node scripts/check-guardrails.js",
    "prebuild": "node scripts/type-check-quick.js && npm run check:routes && npm run check:guardrails",
    "schema:check": "node scripts/schema-check.cjs",
    "check:smooth-rendering": "node scripts/check-smooth-rendering.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:unit": "vitest run src/test/components",
    "test:integration": "vitest run src/test/features",
    "test:hotbite": "vitest run src/test/components/HotBiteAlert.test.tsx src/test/features/hotBiteDetection.test.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@mapbox/mapbox-gl-draw": "^1.5.0",
    "@memberstack/dom": "^2.0.1",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@sentry/nextjs": "^10.15.0",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/auth-ui-react": "^0.4.7",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.57.4",
    "@tanstack/react-query": "^5.90.2",
    "@tanstack/react-query-devtools": "^5.90.2",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "fast-xml-parser": "^5.2.5",
    "idb": "^8.0.3",
    "jose": "^6.1.0",
    "lucide-react": "^0.544.0",
    "mapbox-gl": "^3.14.0",
    "nanoid": "^5.1.5",
    "next": "^15.5.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hot-toast": "^2.6.0",
    "react-use": "^17.6.0",
    "sharp": "^0.34.4",
    "sonner": "^2.0.7",
    "uuid": "^13.0.0",
    "zod": "^4.1.11",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.55.1",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@turf/turf": "^7.2.0",
    "@types/dompurify": "^3.0.5",
    "@types/mapbox__mapbox-gl-draw": "^1.4.9",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.0.3",
    "dompurify": "^3.2.7",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "husky": "^9.1.7",
    "jsdom": "^27.0.0",
    "lint-staged": "^16.2.0",
    "prettier": "^3.6.2",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^3.2.4"
  }
}
